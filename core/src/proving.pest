PROVE_START = _{ "-" }

exact_by_term = { "EXACT" ~ expression }
subset_elim_prop = { "SUBSET" ~ expression ~ "in" ~ expression ~ "<=" ~ expression }

ADMIT = { "ADMIT" }
ADMIT_ALL = {"ADMIT_ALL" }

PROOF = {
    PROVE_START ~ (
        ADMIT | ADMIT_ALL
        | exact_by_term | subset_elim_prop
    )
}
